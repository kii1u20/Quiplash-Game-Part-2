<div v-for="index in Object.keys(prompts).length" :key="index">
    <p class="title">{{JSON.parse(Object.keys(prompts)[index - 1]).text}}</p>
    <div class="boxContainer">
        <div class="box1">
            <p>Answer: {{Object.values(prompts[Object.keys(prompts)[index - 1]].answer1)[0]}}</p>
            <p>Creator: {{Object.keys(prompts[Object.keys(prompts)[index - 1]].answer1)[0]}}</p>
            <p>Votes:</p>
            <div v-for="item of prompts[Object.keys(prompts)[index - 1]].votes">
                <div v-if="Object.values(item.vote)[0] == Object.values(prompts[Object.keys(prompts)[index - 1]].answer1)[0]">
                    <p>
                        {{item.username}}
                    </p>
                </div>
            </div>
        </div>
        <div class="box2">
            <p>Answer: {{Object.values(prompts[Object.keys(prompts)[index - 1]].answer2)[0]}}</p>
            <p>Creator: {{Object.keys(prompts[Object.keys(prompts)[index - 1]].answer2)[0]}}</p>
            <p>Votes:</p>
            <div v-for="item of prompts[Object.keys(prompts)[index - 1]].votes">
                <div v-if="Object.values(item.vote)[0] == Object.values(prompts[Object.keys(prompts)[index - 1]].answer2)[0]">
                    <p>
                        {{item.username}}
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>